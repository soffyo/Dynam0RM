{
  "name": "dynam0rx",
  "version": "1.0.0",
  "description": "AWS DynamoDB Typescript Client",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "git": "git add . && git commit -m",
    "postgit": "git push",
    "version": "git add .",
    "postversion": "git push",
    "prepublishOnly": "npm run test",
    "test": "jest --",
    "dynamodb": "cd dynamodb_local && java -Djava.library.path=./DynamoDBLocal_lib -jar ./DynamoDBLocal.jar -inMemory",
    "test-db": "concurrently -P -k -s \"last\" -n \"DynamoDB,Jest\" -c \"bgBlue.bold.white,bgRed.bold.white\" \"npm:dynamodb\" \"jest {@}\" --"
  },
  "keywords": [
    "aws",
    "dynamodb",
    "typescript",
    "client"
  ],
  "author": "soffyo",
  "license": "GPL-3.0-or-later",
  "devDependencies": {
    "@types/jest": "^28.1.6",
    "@types/node": "^18.6.4",
    "concurrently": "^7.3.0",
    "jest": "^28.1.3",
    "ts-jest": "^28.0.7",
    "typescript": "^4.8.2"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.142.0",
    "@aws-sdk/lib-dynamodb": "^3.145.0",
    "reflect-metadata": "^0.1.13"
  }
}
